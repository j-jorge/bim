{
  "style": {
    "stripe": [
      {
        "instantiate": "ax::Node",
        "anchor-point.x": 0.5,
        "anchor-point.y": 0.5,
        "rotation": -10
      }
    ],

    "stripe_background": [
      "nine-slice/stripe",
      {
        "instantiate": "nine_slices",
        "parent": "stripe",
        "z-order": -10
